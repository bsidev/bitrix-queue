{
  "private": true,
  "scripts": {
    "dev": "encore dev --watch",
    "build": "encore production --progress",
    "lint": "eslint 'assets/**/*.{js,vue}' --quiet --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@symfony/webpack-encore": "^4.4.0",
    "apexcharts": "^3.24.0",
    "autoprefixer": "^9.8.4",
    "axios": "^0.21.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "cache-loader": "^4.1.0",
    "core-js": "^3.6.5",
    "element-ui": "^2.13.2",
    "eslint": "^7.32.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "phpunserialize": "^1.0.1",
    "postcss-loader": "^7.3.3",
    "qs": "^6.9.4",
    "sass": "^1.69.4",
    "sass-loader": "^13.3.2",
    "vue": "^2.7.14",
    "vue-apexcharts": "^1.6.0",
    "vue-i18n": "^8.18.2",
    "vue-json-pretty": "^1.6.5",
    "vue-loader": "^15.11.1",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "yarn-audit-fix": "^10.0.1"
  },
  "eslintConfig": {
    "root": true,
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "parser": "babel-eslint"
    },
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/recommended"
    ],
    "rules": {
      "vue/html-indent": [
        "error",
        4
      ],
      "vue/multi-word-component-names": "off"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "assets/**/*.{js,vue}": [
      "yarn run lint"
    ]
  }
}
